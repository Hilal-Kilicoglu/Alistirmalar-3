def parcala(dizi, solindis, sağindis):
    i=solindis-1
    pivot=dizi[sağindis]
    for j in range(solindis, sağindis):
        if dizi[j]<=pivot:
            i=i+1
            dizi[i], dizi[j]=dizi[j], dizi[i]
    dizi[i+1], dizi[sağindis]=dizi[sağindis], dizi[i+1]
    return i+1
def quick_sort(dizi, solindis, sağindis):
    if solindis<sağindis:
        pivot=parcala(dizi, solindis, sağindis)
        quick_sort(dizi, solindis, pivot-1)
        quick_sort(dizi, pivot+1, sağindis)

siralanmamis_dizi=[10,6,-3,8,15,-7,4]
quick_sort(siralanmamis_dizi, 0, len(siralanmamis_dizi)-1)
print(siralanmamis_dizi)
