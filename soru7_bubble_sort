def bubble_sort(dizi): 
    for i in range(0, len(dizi)-1): #Diziyi son elemanı dışında dolaşan döngü #n-1 tur gerekli
        for j in range(0, len(dizi)-1-i): #En büyük eleman her döngüde en sonda yerine yerleştiği için i çıkardım
            if dizi[j]>dizi[j+1]: 
                (dizi[j], dizi[j+1])=(dizi[j+1], dizi[j])  #yer değiştirme

siralanmamis_dizi=[3,5,-6,19,-4,28] #Fonksiyonu kod içinde çağırmak için bir örnek
print(bubble_sort(siralanmamis_dizi))
